{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/views/login.html"
    },
    {
      "src": "/index.html",
      "dest": "/views/index.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/views/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
